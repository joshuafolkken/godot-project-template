{
    "name": "godot-project-template",
    "version": "1.0.0",
    "scripts": {
        "postinstall": "lefthook install",
        "spellcheck": "cspell \"**/*\"",
        "spellcheck:fix": "cspell \"**/*\" --fix",
        "spellcheck:quiet": "cspell \"**/*\" --quiet",
        "spellcheck:report": "cspell \"**/*\" --report all",
        "pre-commit": "lefthook run pre-commit",
        "pre-push": "lefthook run pre-push"
    },
    "devDependencies": {
        "cspell": "^9.2.0",
        "lefthook": "^1.12.2"
    }
}
